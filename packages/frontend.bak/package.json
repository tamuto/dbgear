{
  "name": "frontend",
  "version": "0.1.0",
  "description": "DBGear Frontend Package",
  "main": "src/main.tsx",
  "scripts": {
    "build": "pnpm run build:tsc && pnpm run build:webpack",
    "build:tsc": "tsc",
    "build:webpack": "webpack --config webpack.config.js --mode development",
    "build:prod": "webpack --config webpack.config.js --mode production",
    "dev": "webpack serve --config webpack.config.js --mode development",
    "watch": "webpack -w --config webpack.config.js --mode development",
    "lint": "eslint --ext .ts,.tsx src --config .eslintrc.yml",
    "lint:fix": "eslint --ext .ts,.tsx src --fix --config .eslintrc.yml",
    "type-check": "tsc --noEmit"
  },
  "author": "tamuto <tamuto@infodb.jp>",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@infodb/uilib": "^0.5.4",
    "@mui/icons-material": "^5.14.13",
    "@mui/lab": "5.0.0-alpha.148",
    "@mui/material": "^5.14.13",
    "@mui/x-data-grid": "^6.16.2",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "axios": "^1.5.1",
    "i18next": "^23.5.1",
    "notistack": "^3.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-i18next": "^13.3.0",
    "react-router-dom": "^6.17.0",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@swc/core": "^1.3.93",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.51.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "swc-loader": "^0.2.3",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
