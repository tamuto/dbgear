{
  "name": "dbgear",
  "version": "0.1.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "setup": "infodb-cli runall setup:*",
    "setup:frontend": "npm install",
    "setup:backend": "poetry install",
    "eslint": "eslint --ext .js --ignore-path .gitignore frontend",
    "build": "infodb-cli runall build:*",
    "build:tsc": "tsc",
    "build:ts": "webpack --config webpack.config.js --mode development",
    "watch": "webpack -w --config webpack.config.js --mode development",
    "serve": "poetry run dbgear serve --project dist/test",
    "check": "poetry run dbgear check --project dist/test",
    "test": "poetry run python -m unittest discover",
    "flake8:src": "poetry run flake8 dbgear",
    "flake8:test": "poetry run flake8 tests"
  },
