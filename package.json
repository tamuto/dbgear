{
  "name": "dbgear",
  "version": "0.1.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "setup": "infodb-cli runall setup:*",
    "setup:frontend": "npm install",
    "setup:backend": "poetry install",
    "eslint": "eslint --ext .js --ignore-path .gitignore frontend",
    "build": "infodb-cli runall build:*",
    "build:es": "infodb-cli es -i frontend/main.js --source-map --emotion frontend/emotion-shim.js",
    "build:minify": "infodb-cli minify -i frontend/index.html",
    "serve": "poetry run dbgear serve --project dist/dbgear",
    "check": "poetry run dbgear check --project dist/dbgear",
    "test": "poetry run python -m unittest discover"
  },
  "author": "tamuto <tamuto@infodb.jp>",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.13.7",
    "@mui/material": "^5.13.7",
    "@mui/x-data-grid": "^6.9.1",
    "axios": "^1.4.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-router-dom": "^6.14.1",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "eslint": "^8.44.0"
  }
}
